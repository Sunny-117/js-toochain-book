{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "indent": ["error", 2],
    "quotes": ["warn", "single"],
    "semi": ["error", "always", {
      "omitLastInOneLineBlock": true,
      "omitLastInOneLineClassBody": true
    }],
    "no-console":"warn"
  },
  "noInlineConfig": false,
  "reportUnusedDisableDirectives": true
}
